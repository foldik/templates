{
  "name": "elm-spa",
  "version": "0.0.1-SNAPSHOT",
  "description": "Elm SPA Starter",
  "main": "index.js",
  "keywords": [],
  "author": "foldik",
  "license": "ISC",
  "scripts": {
    "clean": "rm -rf dist/",
    "format": "elm-format src/",
    "mock-api": "json-server --watch mock-api/db.json --port 9001 --routes mock-api/routes.json",
    "dev": "elm-live src/Main.elm -p 9000 --open --pushstate --proxyPrefix /api --proxyHost http://localhost:9001 --start-page=index.html -- --debug --output=dist/main.js",
    "build": "elm make src/Main.elm --optimize --output=dist/main.js",
    "uglifyjs": "uglifyjs dist/main.js --compress 'pure_funcs=\"F2,F3,F4,F5,F6,F7,F8,F9,A2,A3,A4,A5,A6,A7,A8,A9\",pure_getters=true,keep_fargs=false,unsafe_comps=true,unsafe=true,passes=2' --output=dist/main.js && uglifyjs dist/main.js --mangle --output=dist/main.js"
  },
  "devDependencies": {
    "elm": "^0.19.0",
    "elm-format": "^0.8.1",
    "elm-live": "^3.4.0",
    "json-server": "^0.14.2",
    "uglify-js": "^3.5.9"
  }
}
